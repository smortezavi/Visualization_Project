   "source": [
    "## Fixing commas and floats"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "columns_with_commas = ['dow_price', 'dow_open', \n",
    "                       'dow_high', 'dow_low', \n",
    "                       'gold_price', 'gold_open', \n",
    "                       'gold_high', 'gold_low']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "for cols in columns_with_commas:\n",
    "    final_data[cols] = final_data[cols].map(lambda x:float(x.split(',')[0]+x.split(',')[1]))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## dividing by max to get a relative feel "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "final_data['dow_price_soft_max'] = (final_data.dow_price / final_data.dow_price.max())\n",
    "final_data['gold_price_soft_max'] = (final_data.gold_price / final_data.gold_price.max())\n",
    "final_data['eur_price_soft_max'] = (final_data.eur_price / final_data.eur_price.max())\n",
    "final_data['bit_price_soft_max'] = (final_data.bit_price / final_data.bit_price.max())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "final_data['bit_price_log'] = np.log(final_data.bit_price)"
